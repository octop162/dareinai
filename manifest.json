{
  "manifest_version": 3,
  "name": "だれいない",
  "version": "0.0.1",
  "description": "誰が居ないかを通知する拡張機能。",
  "content_scripts": [{
    "matches": ["https://meet.google.com/*"],
    "js": [
      "content.js"
    ]
  }],
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "icon128.png"
  },
  "permissions": ["storage"]
}